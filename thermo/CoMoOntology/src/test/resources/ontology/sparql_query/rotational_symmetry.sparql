# Returns rotational symmetry number.
PREFIX ontokin: <https://como.cheng.cam.ac.uk/kb/ontokin/>
PREFIX gc: <http://purl.org/gc/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?rotationalSymmetryNumber
WHERE { 
?s gc:isCalculationOn ?rs . 
?rs rdf:type ontokin:RotationalSymmetry . 
?rs ontokin:hasRotationalSymmetryNumber ?rotationalSymmetryNumber .
}