{
    "database": "postgres",
    "workspace": "twa",
    "datasetDirectory": "secondary_substation",
    "dataSubsets": [
        {
            "type": "vector",
            "skip": false,
            "subdirectory": "secondary",  
            "table": "substations_kings_lynn_table",
            "name": "substations_kings_lynn_layer",            
            "ogr2ogrOptions": {
                "inputDatasetOpenOptions": {
                    "X_POSSIBLE_NAMES": "X O Position British National Grid",
                    "Y_POSSIBLE_NAMES": "Y O Position British National Grid"
                }
            },
            "geoServerSettings": {
                "virtualTable": {
                    "name": "substations_kings_lynn_virtual_table",
                    "sql": "@/inputs/config/sql/substation-kings-lynn.sql",
                    "geometry": {
                        "name": "geom",
                        "type": "Point",
                        "srid": 27700
                    }
                },
                "defaultStyle": "point"
            }
        }
    ]
}