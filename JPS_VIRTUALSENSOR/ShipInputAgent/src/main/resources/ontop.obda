[PrefixDeclaration]
geo:		http://www.opengis.net/ont/geosparql#
xsd:		http://www.w3.org/2001/XMLSchema#
disp:       http://www.theworldavatar.com/dispersion/

[MappingDeclaration] @collection [[
mappingId	ships
target		<{datairi}> a geo:Feature; geo:hasGeometry <{geometry_iri}>. <{geometry_iri}> geo:asWKT "{wkt_original}"^^geo:wktLiteral ; disp:hasTime {time}^^xsd:long .
source		SELECT "dbTable"."dataIRI" AS datairi, timeseries.time AS time, ST_AsText(timeseries.value) AS wkt_original, 
            timeseries.tid as tid, ST_AsText(ST_Transform(timeseries.value, 32630)) as wkt_32630, concat("dbTable"."dataIRI",'/geometry',cast(tid as text)) as geometry_iri
            FROM "dbTable", public.get_geometry_table("tableName", "columnName") as timeseries
            WHERE "dbTable"."type"='geometry'
]]