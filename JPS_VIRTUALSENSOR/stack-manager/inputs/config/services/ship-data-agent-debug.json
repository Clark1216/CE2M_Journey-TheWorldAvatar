{
    "ServiceSpec": {
        "Name": "ship-data-agent",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "ghcr.io/cambridge-cares/ship-data-agent:1.0-dev-ship-data-SNAPSHOT",
				"Env": [
					"DATABASE=postgres",
					"JPDA_ADDRESS=0.0.0.0:5005",
					"JPDA_TRANSPORT=dt_socket",
					"DEBUG=ON"
				],
				"Configs": [
					{
						"ConfigName": "postgis"
					},
					{
						"ConfigName": "blazegraph"
					},
					{
						"ConfigName": "ontop"
					},
					{
						"ConfigName": "geoserver"
					}
				],
				"Secrets": [
					{
						"SecretName": "postgis_password"
					},
					{
						"SecretName": "geoserver_password"
					}
				],
                "Mounts": [
					{
                        "Type": "volume",
                        "Source": "misc",
                        "Target": "/misc"
                    }
                ]
            }
        },
		"EndpointSpec": {
			"Ports": [
				{
					"Name": "web",
					"Protocol": "tcp",
					"TargetPort": "5005",
					"PublishedPort": "5010"
				}
			]
		}
    },
    "endpoints": {
        "ship-data-agent": {
            "url": "http://localhost:8080/ShipDataAgent/",
            "externalPath": "/ship-data-agent/"
        }
    }
}