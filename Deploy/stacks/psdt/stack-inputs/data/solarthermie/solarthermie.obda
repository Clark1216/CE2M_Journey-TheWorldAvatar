[PrefixDeclaration]
xsd:		http://www.w3.org/2001/XMLSchema#
ub:         http://www.theworldavatar.com/kg/ontoubemmp/
be:         http://www.theworldavatar.com/kg/ontobuiltenv/
om:         http://www.ontology-of-units-of-measure.org/resource/om-2/
oum:        https://www.theworldavatar.com/kg/ontouom/
gml:        http://www.theworldavatar.com/ontology/ontocitygml/citieskg/OntoCityGML.owl#
db:         http://www.purl.org/oema/enaeq/
bot:        http://w3id.org/bot#
bim:        http://www.theworldavatar.com/ontology/ontobim/OntoBIM.owl#
iri:        https://www.theworldavatar.com/kg/

[MappingDeclaration] @collection [[
mappingId	solarthermie
target		iri:OntoCityGML/Building_{ID} a gml:Building .
            iri:DABGEO/Building_{ID} a db:Building ;
                be:hasOntoCityGMLRepresentation iri:OntoCityGML/Building_{ID} ;
                ub:hasDevice iri:OntoUBEMMP/RoofThermalPlateCollectors_{ID} ;
                ub:hasDevice iri:OntoUBEMMP/RoofThermalTubeCollectors_{ID} ;
                ub:hasDevice iri:OntoUBEMMP/RoofSolarCollectors_{ID} ;
                bot:containsElement iri:OntoBIM/Roof_{ID} .
            iri:OntoUBEMMP/RoofThermalPlateCollectors_{ID} a ub:RoofThermalPlateCollectors ;
                db:producesEnergy iri:OntoUBEMMP/PlateAnnualHeatSupplyQuantity_{ID} .
            iri:OntoUBEMMP/PlateAnnualHeatSupplyQuantity_{ID} a ub:AnnualHeatSupply ;
                om:hasDimension oum:energyPerArea-Dimension ;
                om:hasValue iri:OM/PlateAnnualHeatSupplyValue_{ID} .
            iri:OM/PlateAnnualHeatSupplyValue_{ID} a om:Measure ;
                om:hasUnit oum:kilowattHourPerSquareMetre ;
                om:hasNumericalValue {pow_hz}^^xsd:double ;
                ub:hasCalculationBasis "Calculated at module efficiency of 50%"^^xsd:string .
            iri:OntoUBEMMP/RoofThermalTubeCollectors_{ID} a ub:RoofThermalTubeCollectors ;
                db:producesEnergy iri:OntoUBEMMP/TubeAnnualHeatSupplyQuantity_{ID} .
            iri:OntoUBEMMP/TubeAnnualHeatSupplyQuantity_{ID} a ub:AnnualHeatSupply ;
                om:hasDimension oum:energyPerArea-Dimension;
                om:hasValue iri:OM/TubeAnnualHeatSupplyValue_{ID} .
            iri:OM/TubeAnnualHeatSupplyValue_{ID} a om:Measure ;
                om:hasUnit oum:kilowattHourPerSquareMetre ;
                om:hasNumericalValue {pow_th}^^xsd:double ;
                ub:hasCalculationBasis "Calculated at module efficiency of 60%"^^xsd:string .
            iri:OntoUBEMMP/RoofSolarCollectors_{ID} a ub:RoofSolarCollectors ;
                ub:producesCO2Savings iri:OntoUBEMMP/AnnualCO2SavingsQuantity_{ID} ;
                ub:receivesRadiation iri:OntoUBEMMP/RadabshzQuantity_{ID} .
            iri:OntoUBEMMP/AnnualCO2SavingsQuantity_{ID} a ub:AnnualCO2Savings ;
                om:hasValue iri:OM/AnnualCO2SavingsValue_{ID} .
            iri:OM/AnnualCO2SavingsValue_{ID} a om:Measure ;
                om:hasUnit om:kilogramPerSquareMetre ;
                om:hasNumericalValue {co2_th}^^xsd:double ;
                ub:hasCalculationBasis "CO2 savings per mÂ² at 0.258 kg/kWh - equivalent value compared to natural gas (assuming module efficiency 50%)."^^xsd:string .
            iri:OntoUBEMMP/RadabshzQuantity_{ID} a ub:Radabshz ;
                om:hasDimension om:power-Dimension ;
                om:hasValue iri:OM/RadabshzValue_{ID} .
            iri:OM/RadabshzValue_{ID} a om:Measure ;
                om:hasUnit oum:kilowattHourPerYear ;
                om:hasNumericalValue {radabshz}^^xsd:double .
            iri:OntoBIM/Roof_{ID} a bim:Roof ;
                bim:isHeatSupportSuitable {eignghzi}^^xsd:boolean ;
                bim:isSolarCollectorSuitable {eigngthi}^^xsd:boolean .
source      SELECT oid, geb_id, CONCAT(geb_id, oid) AS ID, pow_hz, pow_th, co2_th, radabshz, eignghzi, eigngthi FROM "solarthermie"
]]

