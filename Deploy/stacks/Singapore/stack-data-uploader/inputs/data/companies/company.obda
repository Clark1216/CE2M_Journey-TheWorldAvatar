mappingId       jurong_island_heat_sources
target          :company/{company_uuid} a ontocompany:Company;
                    rdfs:label "{company}"^^xsd:string;
                    ontocompany:hasSSICCode "{ssic_code}"^^xsd:integer;
                    ontocompany:hasBusinessActivity "{business_activity}"^^xsd:string;
                    ontocompany:isOwnerOf :chemPlant/{company_uuid}.
                :chemPlant/{company_uuid} a ontochemplant:ChemicalPlant;
                    rdfs:label "Chemical_Plant_{company}"^^xsd:string;
                    ontocompany:belongsToIndustry ontocompany:ChemicalIndustry;
                    geo:ehContains :plantitem/{plant_item_uuid}.               
                :plantitem/{plant_item_uuid} a ontocape:PlantItem;
                    ontobuiltenv:hasOntoCityGMLRepresentation cityfurniture:{city_furniture_uuid};
                    ontochemplant:hasIndividualCO2Emission :co2/{plant_item_uuid};
                    ontocompany:hasGeneratedHeat :generatedheat/{plant_item_uuid}.
                :co2/{plant_item_uuid} rdf:type ontochemplant:IndividualCO2Emission;
                    om:hasValue :co2measure/{plant_item_uuid}.
                :co2measure/{plant_item_uuid} om:hasNumericalValue "{co2_emissions}"^^xsd:float;
                    om:hasUnit :co2unit/{plant_item_uuid}. 
                :co2unit/{plant_item_uuid} skos:notation "Tons/year"^^xsd:string.
                :generatedheat/{plant_item_uuid} rdf:type ontochemplant:GeneratedHeat;            
                            om:hasValue :generatedheatmeasure/{plant_item_uuid}.
                :generatedheatmeasure/{plant_item_uuid} om:hasNumericalValue "{heat_emissions}"^^xsd:float;
                    om:hasUnit :generatedheatunit/{plant_item_uuid}. 
                :generatedheatunit/{plant_item_uuid} skos:notation "MW"^^xsd:string.
source          SELECT
                    "company_uuid",
                    "plant_item_uuid",
                    "city_furniture_uuid",
                    "company",
                    "ssic_code",
                    "business_activity",
                    "year_formation",
                    "number_employees",
                    "co2_emissions",
                    "heat_emissions"
                FROM
                    "jurong_island_city_furniture" 
                
 

]]