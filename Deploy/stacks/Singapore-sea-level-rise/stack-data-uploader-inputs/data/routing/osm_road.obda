[PrefixDeclaration]
xsd:    http://www.w3.org/2001/XMLSchema#
rdfs:	http://www.w3.org/2000/01/rdf-schema#
:       https://www.theworldavatar.com/kg/
sea:    https://www.theworldavatar.com/kg/ontosealevel/
be:     https://www.theworldavatar.com/kg/ontobuiltenv/
ic:     http://ontology.eil.utoronto.ca/icontact.owl#
sch:    https://schema.org/
dbo:    https://dbpedia.org/ontology/
landplot: https://www.theworldavatar.com/kg/landplot/
ost:    https://www.theworldavatar.com/kg/ontostreet/
geo:    http://www.opengis.net/ont/geosparql#
osm: https://w3id.org/openstreetmap/terms#
openstreetmap: https://www.openstreetmap.org/

[MappingDeclaration] @collection [[
mappingId	OSM Road
target		openstreetmap:way/{osm_id} a osm:Way ;
                a osm:{road_type} ;
                a geo:Feature ;
                osm:hasLength "{total_length_m}";
                osm:name "{name}";
                osm:onewayvalue "{oneway}"; 
                osm:maxspeed "{maxspeed_forward}";
                geo:hasGeometry "{geom_comb}"^^geo:wktLiteral.
source		SELECT osm_id, name, total_length_m, road_type, oneway, maxspeed_forward, maxspeed_backward, ST_ASTEXT(geom) as geom_comb FROM osm_road_mat_view
]]
