{
    "name": "sealevelprojections",
    "database": "postgres",
    "workspace": "twa",
    "datasetDirectory": "sealevelprojections",
    "dataSubsets": [
        {
			"name": "sealevelprojections",
            "type": "vector",
			"subdirectory": "sealevelshape",
            "sql": "@/inputs/data/sealevelprojections/sealevelshape/slrprojections.sql",
            "ogr2ogrOptions": {

                                "sridIn": "EPSG:4326",
                "otherOptions": {
                    "-nlt": [
                        "MULTIPOLYGON"
                    ]
                }
            },
        
            "geoServerSettings": {
                "virtualTable": {
                    "name": "sealevelprojections",
                    "escapeSql": false,
                    "geometry": {
                        "name": "wkb_geom",
                        "type": "MultiPolygon",
                        "srid": 4326
                    }
                },
                "defaultStyle": "multipolygon"
            }
        }
    ],
    "mappings": [
        "sealevelprojections.obda"
    ]
}