prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix ems: <https://www.theworldavatar.com/kg/ontoems/>

SELECT ?label ?value ?unit
WHERE {
  { BIND ("Type" AS ?label )
    [IRI] rdf:type ?value_ini .
    BIND (replace(str(?value_ini), str(ems:), '') as ?value)
  } UNION { 
    BIND ("Name" AS ?label )
    [IRI] rdfs:label ?value .
  } UNION { 
    BIND ("Data source" AS ?label )
    [IRI] ems:dataSource ?value .
  } UNION { 
    BIND ("Identifier" AS ?label )
    [IRI] ems:hasIdentifier ?value .
  } UNION { 
    BIND ("Elevation" AS ?label )
    BIND ("m" AS ?unit )
    [IRI] ems:hasObservationElevation ?value .
  }
}
