{
  "ServiceSpec": {
    "Name": "pips-agent",
    "TaskTemplate": {
      "ContainerSpec": {
        "Image": "pips-agent:1.0.0",
        "Env": [
          "CLIENT_ID=<client ID>",
          "KEYCLOAK_REALM_PATH=<keycloak endpoint>"
				],
        "Configs": [
          {
              "ConfigName": "postgis"
          }
      ],
      "Secrets": [
          {
              "SecretName": "postgis_password"
          }
      ]
      }
    }
  },
  "endpoints": {
    "ui": {
      "url": "http://localhost:8080/pips-agent/",
      "externalPath": "/pips-agent/"
    }
  }
}