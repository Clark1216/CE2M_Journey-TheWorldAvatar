{
    "ServiceSpec": {
        "Name": "dh-optimisation-trigger-agent",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "ghcr.io/cambridge-cares/dh-optimisation-trigger-agent:0.0.1-SNAPSHOT",
                "Command": [
                    "/bin/bash", "./delayed_start.sh"
                ],
                "Mounts": [
                    {
                        "Type": "bind",
                        "Source": "/home/caresuser/TWA-git/Agents/DistrictHeatingOptimisationTriggerAgent/resources/triples",
                        "Target": "/app/resources/triples"
                    }
                ],
                "Env": [
                    "NAMESPACE=kb",
                    "FORECASTING_AGENT=https://www.theworldavatar.com/resource/agents/Service__ForecastingAgent/Service",
                    "DH_OPTIMISATION_AGENT=https://www.theworldavatar.com/resource/agents/Service__DHOptimisationAgent/Service",
                    "EMISSION_ESTIMATION_AGENT=https://www.theworldavatar.com/resource/agents/Service__DHEmissionAgent/Service"
                ],
                "Configs": [
                    {
                        "ConfigName": "blazegraph"
                    }
                ]
            }
        }
    },
    "endpoints": {
        "rest": {
            "url": "http://localhost:5000/",
            "externalPath": "/dhTriggerAgent/"
        }
    }
}