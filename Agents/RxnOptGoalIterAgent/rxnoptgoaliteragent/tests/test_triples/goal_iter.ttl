@prefix rdfs:            <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:            <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:             <http://www.w3.org/2001/XMLSchema#> .
@prefix saref:             <https://saref.etsi.org/core/> .
@prefix om:             <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix ontorxn:         <https://raw.githubusercontent.com/cambridge-cares/TheWorldAvatar/main/JPS_Ontology/ontology/ontoreaction/OntoReaction.owl#> .
@prefix ontogoal:        <https://raw.githubusercontent.com/cambridge-cares/TheWorldAvatar/main/JPS_Ontology/ontology/ontogoal/OntoGoal.owl#> .
@prefix :                <https://www.example.com/triplestore/ontogoal/rxnopt/> .

@prefix rxnopt:          <http://www.theworldavatar.com/resource/plans/RxnOpt/> .

:GoalSet_1 
    a ontogoal:GoalSet;
    ontogoal:hasGoal :Goal_1;
    ontogoal:hasGoal :Goal_2;
    ontogoal:hasRestriction :Restriction_1;
.

:Goal_1
    a ontogoal:Goal;
    ontogoal:desiresGreaterThan ontorxn:Yield;
    ontogoal:hasPlan rxnopt:rxnoptplan;
.

:Goal_1
    a ontogoal:Goal;
    ontogoal:desiresLessThan ontorxn:RunMaterialCost;
    ontogoal:hasPlan rxnopt:rxnoptplan;
.

:Restriction_1
    a ontogoal:Restriction;
    ontogoal:cycleAllowance 5;
    ontogoal:deadline "2100-01-01T00:00:00"^^xsd:dateTime;
.
