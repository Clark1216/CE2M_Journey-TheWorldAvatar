{
  "ServiceSpec": {
    "Name": "trajectoryqueryagent",
    "TaskTemplate": {
      "ContainerSpec": {
        "Image": "ghcr.io/cambridge-cares/trajectoryqueryagent:1.0.0", 
        "Mounts": [
          {
              "Type": "bind",
              "Source": "../../../../../../Agents/TrajectoryQueryAgent/trajectoryqueryagent/src/main/resources",
              "Target": "/resources"
          }
        ],
        "Configs": [
          {
            "ConfigName": "blazegraph"
          },
          {
            "ConfigName": "postgis"
          },
          {
            "ConfigName": "geoserver"
          }
        ],
        "Secrets": [
          {
            "SecretName": "postgis_password"
          },
          {
            "SecretName": "geoserver_password"
          }
        ]
      }
    }
  },
  "endpoints": {
    "isochroneagent": {
      "url": "http://localhost:8080/trajectoryqueryagent/",
      "externalPath": "/trajectoryqueryagent/"
    }
  }
}