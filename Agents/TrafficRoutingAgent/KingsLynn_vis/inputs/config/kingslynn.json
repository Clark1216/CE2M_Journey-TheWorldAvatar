{
    "name": "kingslynn",
    "database": "postgres",
    "workspace": "kingslynn",
    "datasetDirectory": "kingslynn",
    "dataSubsets": [        
        {
            "type": "OSMRouting",
            "skip": true,
            "schema": "public",
            "subdirectory": "routing",
            "name": "routing",
            "verticesGeoServerSettings": {
                "virtualTable": {
                    "name": "routing",
                    "sql": "@/inputs/data/kingslynn/sql/nearest_vertex.sql",
                    "escapeSql": false,
                    "geometry": {
                        "name": "the_geom",
                        "type": "Point",
                        "srid": 4326
                    },
                    "parameters": [
                        {
                            "name": "lon",
                            "defaultValue": "1",
                            "regexpValidator": "^[\\d\\.\\+-eE]+$"
                        },
                        {
                            "name": "lat",
                            "defaultValue": "10",
                            "regexpValidator": "^[\\d\\.\\+-eE]+$"
                        }
                    ]
                }
            },            
            "waysGeoServerSettings": {
                "virtualTable": {
                    "name": "routing",
                    "sql": "@/inputs/data/kingslynn/sql/roaddisplay.sql",
                    "escapeSql": false,
                    "geometry": {
                        "name": "the_geom",
                        "type": "MultiLineString",
                        "srid": 4326
                    }
                }
            },
            "poiGeoServerSettings": {
                "virtualTable": {
                    "name": "routing",
                    "sql": "@/inputs/data/kingslynn/sql/shortest_paths.sql",
                    "escapeSql": false,
                    "geometry": {
                        "name": "the_geom",
                        "type": "MultiLineString",
                        "srid": 4326
                    },
                    "parameters": [
                        {
                            "name": "source",
                            "defaultValue": "1",
                            "regexpValidator": "^[\\d]+$"
                        },
                        {
                            "name": "target",
                            "defaultValue": "10",
                            "regexpValidator": "^[\\d]+$"
                        }
                    ]
                }
            }
        },        
        {
            "type": "raster",
            "name": "flood",
            "skip": false,
            "subdirectory": "flood",
            "sql":"@/inputs/data/kingslynn/sql/flood.sql",
            "geoServerSettings": {
                "layerSettings": {
                    "defaultStyle": "elevation"
                }
            }
        },

        {
            "type": "raster",
            "name": "elevation",
            "skip": false,
            "subdirectory": "elevation",
            "sql":"@/inputs/data/kingslynn/sql/elevation.sql",
            "geoServerSettings": {
                "layerSettings": {
                    "defaultStyle": "elevation"
                }
            }
        },
        {
            "type": "raster",
            "name": "population",
            "skip": false,
            "subdirectory": "population",
            "sql":"@/inputs/data/kingslynn/sql/tripcentrality.sql",
            "geoServerSettings": {
                "layerSettings": {
                    "defaultStyle": "elevation"
                }
            }
        },
        {
            "type": "tabular",
            "skip": false,
            "sql":"@/inputs/data/kingslynn/sql/grid_primary_site.sql",
            "table": "grid_primary_site",
            "subdirectory": "grid_primary_site"
        }
    ]
}
