PREFIX ontoagent:           <http://www.theworldavatar.com/ontology/ontoagent/MSM.owl#>
PREFIX ontodoe:             <https://github.com/cambridge-cares/TheWorldAvatar/blob/develop/JPS_Ontology/ontology/ontodoe/OntoDoE.owl#>
PREFIX ontoderivation:      <https://github.com/cambridge-cares/TheWorldAvatar/blob/develop/JPS_Ontology/ontology/ontoderivation/OntoDerivation.owl#>
PREFIX rdf:                 <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX doeagent:            <http://www.theworldavatar.com/kb/agents/Service__DoE#>
PREFIX doe:                 <https://theworldavatar.com/kb/ontodoe/DoE_1/>
PREFIX :                    <http://www.theworldavatar.com/kb/ontoderivation/derivation_doe/>

INSERT DATA {
    :Derivation_doe
        rdf:type ontoderivation:Derivation;
        ontoderivation:isDerivedUsing doeagent:Service;
        ontoderivation:isDerivedFrom doe:Strategy_1;
        ontoderivation:isDerivedFrom doe:Domain_1;
        ontoderivation:isDerivedFrom doe:SystemResponse_1;
        ontoderivation:isDerivedFrom doe:SystemResponse_2;
        ontoderivation:isDerivedFrom doe:HistoricalData_1;
    .

    doe:NewExperiment_1
        ontoderivation:belongsTo :Derivation_doe;
    .
}