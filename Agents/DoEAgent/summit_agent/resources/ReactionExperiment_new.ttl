PREFIX OntoCAPERxn:<http://www.theworldavatar.com/ontology/ontocape/material/substance/reaction_mechanism.owl#>
PREFIX OntoCAPEMat:<http://www.theworldavatar.com/ontology/ontocape/material/material.owl#>
PREFIX OntoSpecies:<http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#>
PREFIX OntoKin:	<http://www.theworldavatar.com/ontology/ontokin/OntoKin.owl#>
PREFIX OntoDoE:		<https://github.com/cambridge-cares/TheWorldAvatar/blob/develop/JPS_Ontology/ontology/ontodoe/OntoDoE.owl#>
PREFIX OntoRxn:	<https://github.com/cambridge-cares/TheWorldAvatar/blob/develop/JPS_Ontology/ontology/ontorxn/OntoRxn.owl#>
PREFIX om:			<http://www.ontology-of-units-of-measure.org/resource/om-2/>
PREFIX OntoUoM:	<http://theworldavatar.com/resource/ontouom/>
PREFIX SinglePhase:<https://theworldavatar.com/kb/ontorxn/SinglePhase/>
PREFIX ChemRxn:	<https://theworldavatar.com/kb/ontorxn/ChemRxn_1/>
PREFIX exp1:			<https://theworldavatar.com/kb/ontorxn/ReactionExperiment_1/>
PREFIX :			<https://theworldavatar.com/kb/ontorxn/ReactionExperiment_new/>

INSERT DATA {
	exp1:RxnExp_1
		OntoRxn:hasVariation :RxnExp_1;
	.

	:RxnExp_1
		rdf:type OntoRxn:ReactionVariation;
		OntoRxn:isVariationOf exp1:RxnExp_1;
		OntoRxn:hasResTime :ResidenceTime_1;
		OntoRxn:hasRxnTemperature :RxnTemperature_1;
		OntoRxn:hasStoichiometryRatio :StoiRatio_2;
		OntoRxn:hasStoichiometryRatio :StoiRatio_3;
		OntoRxn:hasPerformanceIndicator :Yield_1;
		OntoRxn:hasPerformanceIndicator :RunMaterialCost_1;
	.

	:ResidenceTime_1
		rdf:type om:Duration;
		rdf:type OntoRxn:ResidenceTime; 
		om:hasPhenomenon :RxnExp_1;
		om:hasValue :ResidenceTime_1_Measure_1;
	.

	:ResidenceTime_1_Measure_1
		rdf:type om:Measure;
		om:hasUnit om:minute-Time;
		om:hasNumericalValue ""^^xsd:double;
	.

	:RxnTemperature_1
		rdf:type om:CelsiusTemperature;
		rdf:type OntoRxn:ReactionTemperature; 
		om:hasPhenomenon :RxnExp_1;
		om:hasValue :RxnTemperature_1_Measure_1;
	.

	:RxnTemperature_1_Measure_1
		rdf:type om:Measure;
		om:hasUnit om:degreeCelsius;
		om:hasNumericalValue ""^^xsd:double;
	.

	:Yield_1
		rdf:type OntoRxn:PerformanceIndicator;
		rdf:type OntoRxn:Yield ; 
		om:hasPhenomenon :RxnExp_1;
	.

	:RunMaterialCost_1
		rdf:type OntoRxn:PerformanceIndicator;
		rdf:type OntoRxn:RunMaterialCost ; 
		om:hasPhenomenon :RxnExp_1;
	.

	:InputChemical_1
		rdf:type OntoRxn:InputChemical;
		OntoCAPEMat:thermodynamicBehavior SinglePhase:Phase_1;
	.

	:InputChemical_2
		rdf:type OntoRxn:InputChemical;
		OntoCAPEMat:thermodynamicBehavior SinglePhase:Phase_2;
	.

	:InputChemical_3
		rdf:type OntoRxn:InputChemical;
		OntoCAPEMat:thermodynamicBehavior SinglePhase:Phase_3;
	.

	:OutputChemical_4
		rdf:type OntoRxn:OutputChemical;
		OntoCAPEMat:thermodynamicBehavior SinglePhase:Phase_4;
	.

	:StoiRatio_2
		rdf:type OntoRxn:StoichiometryRatio;
		OntoDoE:positionalID "2"^^xsd:integer;
		om:hasPhenomenon :RxnExp_1;
		OntoRxn:indicatesMultiplicityOf :InputChemical_2;
		om:hasValue :StoiRatio_2_Measure_1;
	.

	:StoiRatio_2_Measure_1
		rdf:type om:Measure;
		om:hasUnit om:one;
		om:hasNumericalValue ""^^xsd:double;
	.

	:StoiRatio_3
		rdf:type OntoRxn:StoichiometryRatio;
		OntoDoE:positionalID "3"^^xsd:integer;
		om:hasPhenomenon :RxnExp_1;
		OntoRxn:indicatesMultiplicityOf :InputChemical_3;
		om:hasValue :StoiRatio_3_Measure_1;
	.

	:StoiRatio_3_Measure_1
		rdf:type om:Measure;
		om:hasUnit om:one;
		om:hasNumericalValue ""^^xsd:double;
	.
}