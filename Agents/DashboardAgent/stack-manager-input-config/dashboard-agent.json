{
    "ServiceSpec": {
      "Name": "dashboard-agent",
      "TaskTemplate": {
        "ContainerSpec": {
          "Image": "dashboard-agent:1.0.0",
          "Mounts": [
            {
              "Type": "bind",
              "Source": "/home/common/Codes/TheWorldAvatar/Agents/DashboardAgent/config",
              "Target": "/usr/local/tomcat/config"
            }],
          "Configs": [
            {
              "ConfigName": "blazegraph"
            },
            {
              "ConfigName": "postgis"
            }
          ],
          "Secrets": [
            {
              "SecretName": "blazegraph_password"
            }, {
              "SecretName": "postgis_password"
            }
          ]
        }
      }
    },
    "endpoints": {
      "rest": {
        "url": "http://localhost:8080/dashboard-agent/",
        "externalPath": "/dashboard-agent/"
      }
    }
  }