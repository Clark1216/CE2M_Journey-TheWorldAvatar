{
    "goals": {
        "hpo": true,
        "contour_plot": false,
        "best_trial_retraining": true,
        "transfer_learning": false,
        "predict": false
    },
    "numerical_settings": {
        "seed": 1,
        "cudnn_deterministic": true,
        "cudnn_benchmark": false
    },
    "dataset": {
        "src": "./data/processed/qsar_aquatic_toxicity.csv",
        "z-stand": "True",
        "x_column": [
            "TPSA(Tot)",
            "SAacc",
            "H-050",
            "MLOGP",
            "RDCHI",
            "GATS1p",
            "nN",
            "C-040"
        ],
        "y_column": [
            "LC50 [-LOG(mol/L)]"
        ],
        "split": [
            0.7,
            0.15,
            0.15
        ]
    },
    "model": {
        "name": "SVR",
        "model_specific": {
            "kernel": "rbf",
            "gamma_structural": {
                "type": "loguniform",
                "low": 0.001,
                "high": 20.0
            },
            "C": {
                "type": "loguniform",
                "low": 0.1,
                "high": 20.0
            },
            "epsilon": {
                "type": "loguniform",
                "low": 0.0001,
                "high": 1.0
            }
        }
    },
    "training": {
        "metric": "mse",
        "cross_validation": false,
        "direction": "minimize",
        "trials": 100,
        "study_name": "svr_test_aq_tox",
        "storage": "sqlite:///svr_test_aq_tox.db",
        "storage_timeout": 1000,
        "load_if_exists": true,
        "jobs": 1
    },
    "post_processing": {
        "contour_plot_alt_dir": null,
        "z_transform_inverse_prediction": true,
        "regression_plot": true,
        "transfer": false
    }
}