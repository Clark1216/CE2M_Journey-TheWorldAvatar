[PrefixDeclaration]
xsd:    http://www.w3.org/2001/XMLSchema#
bot:    https://w3id.org/bot#
env: 	https://www.theworldavatar.com/kg/ontobuiltenv/
twa:    https://www.theworldavatar.com/kg/

[MappingDeclaration] @collection [[
mappingId	gfa
target		twa:Building/{building_iri} env:hasGFA {gfa}^^xsd:float .
source      SELECT realval AS gfa, strval AS building_iri 
            FROM citydb.cityobject_genericattrib
            WHERE attrname = 'GFA' OR attrname = 'uuid'
]]