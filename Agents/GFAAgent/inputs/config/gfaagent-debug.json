{
    "ServiceSpec": {
        "Name": "gfaagent",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "ghcr.io/cambridge-cares/gfaagent:1.2.0",
                "Env": [
                    "STACK_NAME=${STACK_NAME}",
                    "DATABASE=postgres",
                    "osmSchema=osm",
                    "osmPoints=points",
                    "osmPolygons=polygons",
                    "JPDA_ADDRESS=0.0.0.0:5005",
                    "JPDA_TRANSPORT=dt_socket",
                    "DEBUG=ON"
                ],
                "Mounts": [
                    {
                        "Type": "bind",
                        "Source": "../../../../../../Agents/GFAAgent/src/main/resources",
                        "Target": "/resources"
                    }
                ],
                "Configs": [
                    {
                        "ConfigName": "blazegraph"
                    },
                    {
                        "ConfigName": "postgis"
                    },
                    {
                        "ConfigName": "geoserver"
                    },
                    {
                        "ConfigName": "ontop"
                    }
                ],
                "Secrets": [
                    {
                        "SecretName": "postgis_password"
                    },
                    {
                        "SecretName": "geoserver_password"
                    }
                ]
            }
        },
        "EndpointSpec": {
            "Ports": [
                {
                    "Name": "web",
                    "Protocol": "tcp",
                    "TargetPort": "5005",
                    "PublishedPort": "5005"
                }
            ]
        }
    },
    "endpoints": {
        "gfaagent": {
            "url": "http://localhost:8080/GFAAgent/",
            "externalPath": "/gfaagent/"
        }
    }
}