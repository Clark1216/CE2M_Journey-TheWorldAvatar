[PrefixDeclaration]
xsd:    http://www.w3.org/2001/XMLSchema#
bot:    https://w3id.org/bot#
env: 	https://www.theworldavatar.com/kg/ontobuiltenv/
twa:    https://www.theworldavatar.com/kg/

[MappingDeclaration] @collection [[

mappingId	building_floor
target		twa:Building/{strval} env:hasNumberOfFloors twa:NumberOfFloors/{strval} .
            twa:NumberOfFloors/{building_iri} a env:FloorCat{storeys_above_ground_cat} ;
            env:hasValue {storeys_above_ground}^^xsd:integer .
            

source      SELECT 
            cb.storeys_above_ground, 
            cb.storeys_above_ground_cat, 
            ccg.strval
            FROM citydb.building cb, citydb.cityobject_genericattrib ccg
            WHERE cb.id = ccg.cityobject_id AND ccg.attrname = 'uuid' AND strval IS NOT NULL
]]