[PrefixDeclaration]
xsd:    http://www.w3.org/2001/XMLSchema#
bot:    https://w3id.org/bot#
env: 	https://www.theworldavatar.com/kg/ontobuiltenv/
twa:    https://www.theworldavatar.com/kg/

[MappingDeclaration] @collection [[

mappingId	building_floor
target		twa:Building/{building_iri} env:hasNumberOfFloors twa:NumberOfFloors/{building_iri} .
            twa:NumberOfFloors/{building_iri} a env:FloorCat{storeys_above_ground_cat} ;
            env:hasValue {floors}^^xsd:integer .
            

source      SELECT cb.storeys_above_ground AS floors, cb.storeys_above_ground_cat, ccg.strval AS building_iri
            FROM citydb.building cb, citydb.cityobject_genericattrib ccg
            WHERE cb.id = ccg.cityobject_id AND ccg.attrname = 'uuid'
]]