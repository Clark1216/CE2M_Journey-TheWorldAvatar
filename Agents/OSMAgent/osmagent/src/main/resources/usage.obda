[PrefixDeclaration]
xsd:    http://www.w3.org/2001/XMLSchema#
bot:    https://w3id.org/bot#
: 	https://www.theworldavatar.com/kg/ontobuiltenv/

[MappingDeclaration] @collection [[
mappingId	usage
target		<{building_iri}> a bot:Building ;
                :hasPropertyUsage <{propertyusage_iri}> .
            <{propertyusage_iri}> a :{ontobuilt} ;
                :hasUsageShare {usageshare}^^xsd:float .
source      SELECT DISTINCT building_iri, ontobuilt, propertyusage_iri, usageshare
            FROM points
            WHERE building_iri is not null and propertyusage_iri is not null
            UNION
            SELECT DISTINCT building_iri, ontobuilt, propertyusage_iri, usageshare
            FROM polygons
            WHERE building_iri is not null and propertyusage_iri is not null
]]
