[PrefixDeclaration]
geo:		http://www.opengis.net/ont/geosparql#
xsd:		http://www.w3.org/2001/XMLSchema#

[MappingDeclaration] @collection [[
mappingId   weather_location
target      <{iri}> a geo:Geometry;
                    geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/4326> {wkt}"^^geo:wktLiteral .
source      SELECT ST_AsText(wkb_geometry) as wkt, iri FROM weatherstations
]]