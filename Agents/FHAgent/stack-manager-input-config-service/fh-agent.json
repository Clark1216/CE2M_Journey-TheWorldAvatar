{
    "ServiceSpec": {
      "Name": "asset-manager-agent",
      "TaskTemplate": {
        "ContainerSpec": {
          "Image": "docker.io/library/fh-agent:1.0.1",
          "Configs": [
            {
              "ConfigName": "blazegraph"
            },
            {
                "ConfigName": "postgis"
            }
        ],
        "Secrets": [
            {
                "SecretName": "blazegraph_password"
            },
            {
                "SecretName": "postgis_password"
            }
        ]
        }
      }
    },
    "endpoints": {
      "ui": {
        "url": "http://localhost:8080/fh-agent/",
        "externalPath": "/fh-agent/"
      }
    }
  }