{
  "ServiceSpec": {
    "Name": "bms-query-agent",
    "TaskTemplate": {
      "ContainerSpec": {
        "Image": "docker.io/library/bms-query-agent:1.0.0-SNAPSHOT",
        "Env": [
          "CLIENT_PROPERTIES=/root/client.properties"
        ],
        "Configs": [
          {
            "ConfigName": "blazegraph"
          },
          {
            "ConfigName": "ontop"
          },
          {
            "ConfigName": "postgis"
          }
        ],
        "Secrets": [
          {
            "SecretName": "postgis_password"
          }
        ]
      }
    }
  },
  "endpoints": {
    "ui": {
      "url": "http://localhost:8080/bms-query-agent/",
      "externalPath": "/bms-query-agent/"
    }
  }
}