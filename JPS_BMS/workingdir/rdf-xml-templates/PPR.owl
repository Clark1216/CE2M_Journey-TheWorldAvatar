<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:system="http://www.theworldavatar.com/OntoCAPE/OntoCAPE/upper_level/system.owl#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:Eco-industrialPark="http://www.theworldavatar.com/ontology/ontoeip/ecoindustrialpark/EcoIndustrialPark.owl#"
  xmlns:ns1="http://www.theworldavatar.com/OntoCAPE/OntoCAPE/chemical_process_system/CPS_realization/process_control_equipment/measuring_instrument.owl#"
  xmlns:BuildingsLayer="http://www.jparksimulator.com/BuildingsLayer.owl#"
  xmlns:ns2="http://www.theworldavatar.com/OntoCAPE/OntoCAPE/chemical_process_system/CPS_realization/process_control_system.owl#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:SensorOntology="http://www.theworldavatar.com/SensorOntology.owl#"
>
  <owl:Ontology rdf:about="http://www.theworldavatar.com/BMS/PPR.owl">
    <owl:imports rdf:resource="http://www.theworldavatar.com/SensorOntology.owl"/>
    <owl:imports rdf:resource="http://www.jparksimulator.com/BuildingsLayer.owl"/>
    <owl:imports rdf:resource="http://www.theworldavatar.com/OntoEIP/Eco-industrialPark.owl"/>
  </owl:Ontology>
  <BuildingsLayer:Room rdf:about="http://www.theworldavatar.com/BMS/PPR.owl#PowderProcessingRoom">
    <system:hasDirectSubsystem>
      <ns1:Q-Sensor rdf:about="http://www.theworldavatar.com/BMS/PPR.owl#PPR_RH_sensor1">
        <Eco-industrialPark:hasIRI rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/BMS/PPR_RH_sensor1.owl</Eco-industrialPark:hasIRI>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
      </ns1:Q-Sensor>
    </system:hasDirectSubsystem>
    <system:hasSubsystem>
      <owl:NamedIndividual rdf:about="http://www.theworldavatar.com/BMS/PPR.owl#WFH-09">
        <Eco-industrialPark:hasIRI rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/BMS/WFH-09.owl</Eco-industrialPark:hasIRI>
      </owl:NamedIndividual>
    </system:hasSubsystem>
    <system:hasSubsystem>
      <owl:NamedIndividual rdf:about="http://www.theworldavatar.com/BMS/PPR.owl#HVAV-S/7-15">
        <Eco-industrialPark:hasIRI rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/BMS/HVAV-S7-15.owl</Eco-industrialPark:hasIRI>
      </owl:NamedIndividual>
    </system:hasSubsystem>
    <system:hasSubsystem>
      <owl:NamedIndividual rdf:about="http://www.theworldavatar.com/BMS/PPR.owl#VAV-E/7-18A">
        <Eco-industrialPark:hasIRI rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/BMS/VAV-E7-18A.owl</Eco-industrialPark:hasIRI>
        <rdf:type rdf:resource="http://www.theworldavatar.com/OntoCAPE/OntoCAPE/chemical_process_system/CPS_realization/process_control_system.owl#ProcessControlSystem"/>
      </owl:NamedIndividual>
    </system:hasSubsystem>
    <system:hasDirectSubsystem>
      <owl:NamedIndividual rdf:about="http://www.theworldavatar.com/BMS/PPR.owl#PPR_RT_sensor1">
        <rdf:type rdf:resource="http://www.theworldavatar.com/OntoCAPE/OntoCAPE/chemical_process_system/CPS_realization/process_control_equipment/measuring_instrument.owl#T-Sensor"/>
        <Eco-industrialPark:hasIRI rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/BMS/PPR_RT_sensor1.owl</Eco-industrialPark:hasIRI>
      </owl:NamedIndividual>
    </system:hasDirectSubsystem>
    <system:hasProperty>
      <owl:NamedIndividual rdf:about="http://www.theworldavatar.com/BMS/PPR.owl#PPR_DiffPressure">
        <rdf:type rdf:resource="http://www.jparksimulator.com/BuildingsLayer.owl#RoomDifferentialPressure"/>
        <SensorOntology:isObservedBy rdf:resource="http://www.theworldavatar.com/BMS/PPR.owl#PPR_RDP_sensor1"/>
      </owl:NamedIndividual>
    </system:hasProperty>
    <system:hasProperty>
      <owl:NamedIndividual rdf:about="http://www.theworldavatar.com/BMS/PPR.owl#PPR_Temperature">
        <SensorOntology:isObservedBy rdf:resource="http://www.theworldavatar.com/BMS/PPR.owl#PPR_RT_sensor1"/>
        <rdf:type rdf:resource="http://www.jparksimulator.com/BuildingsLayer.owl#RoomTemperature"/>
      </owl:NamedIndividual>
    </system:hasProperty>
    <system:hasDirectSubsystem>
      <ns1:P-Sensor rdf:about="http://www.theworldavatar.com/BMS/PPR.owl#PPR_RDP_sensor1">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
        <Eco-industrialPark:hasIRI rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/BMS/PPR_RDP_sensor1.owl</Eco-industrialPark:hasIRI>
      </ns1:P-Sensor>
    </system:hasDirectSubsystem>
    <system:hasSubsystem>
      <owl:NamedIndividual rdf:about="http://www.theworldavatar.com/BMS/PPR.owl#VAV-S/7-15">
        <Eco-industrialPark:hasIRI rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/BMS/VAV-S7-15.owl</Eco-industrialPark:hasIRI>
        <rdf:type rdf:resource="http://www.theworldavatar.com/OntoCAPE/OntoCAPE/chemical_process_system/CPS_realization/process_control_system.owl#ProcessControlSystem"/>
      </owl:NamedIndividual>
    </system:hasSubsystem>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <system:hasProperty>
      <BuildingsLayer:RelativeHumidity rdf:about="http://www.theworldavatar.com/BMS/PPR.owl#PPR_RelativeHumidity">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
        <SensorOntology:isObservedBy rdf:resource="http://www.theworldavatar.com/BMS/PPR.owl#PPR_RH_sensor1"/>
      </BuildingsLayer:RelativeHumidity>
    </system:hasProperty>
  </BuildingsLayer:Room>
</rdf:RDF>
